            android:value="bj16oBtQ3VAvqPbGtEHB69" />

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="7b3d4abbc6ac33c33b7d97985e8baf4f" />

        <service android:name="com.amap.api.location.APSService" />

        <!-- 用户自定义服务继承自GTIntentService,作为SDK与APP桥梁服务,用来接收各种消息和命令回复-->
        <service
            android:name="com.twt.service.push.WBYIntentService"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <!-- 用户自定义服务名 -->
        <service
            android:name="com.twt.service.push.WBYPushService"
            android:exported="true"
            android:label="PushService"
            android:process=":pushservice" />

<!--        <service android:name=".message.WBYHmsMessageService" android:exported="false">-->
<!--            <intent-filter>-->
<!--                <action android:name="com.huawei.push.action.MESSAGING_EVENT"/>-->
<!--            </intent-filter>-->
